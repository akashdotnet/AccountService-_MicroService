{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Debug",
        "Microsoft.Hosting.Lifetime": "Debug"
      }
    },
    "Enrich": [
      "WithMachineName",
      "WithCorrelationId",
      "WithEnvironmentUserName"
    ],
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "outputTemplate": "[{MachineName} | {ApplicationName} | {Timestamp:o} | {CorrelationId} | {Level:u3}] {Message:lj}{NewLine}{Exception}"
        }
      }
    ],
    "Properties": {
      "ApplicationName": "Account Service"
    }
  }
}
